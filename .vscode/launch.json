{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Finalize Feature",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/dev_assistant.py",
      "args": ["finalize-feature"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {},
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Start Next Feature",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/dev_assistant.py",
      "args": ["start-next-feature"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {},
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
